#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/escodegen@1.8.1/node_modules/escodegen/bin/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/escodegen@1.8.1/node_modules/escodegen/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/escodegen@1.8.1/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/escodegen@1.8.1/node_modules/escodegen/bin/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/escodegen@1.8.1/node_modules/escodegen/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/escodegen@1.8.1/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../escodegen/bin/esgenerate.js" "$@"
else
  exec node  "$basedir/../escodegen/bin/esgenerate.js" "$@"
fi
