#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/delete-empty@3.0.0/node_modules/delete-empty/bin/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/delete-empty@3.0.0/node_modules/delete-empty/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/delete-empty@3.0.0/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/delete-empty@3.0.0/node_modules/delete-empty/bin/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/delete-empty@3.0.0/node_modules/delete-empty/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/delete-empty@3.0.0/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../delete-empty/bin/cli.js" "$@"
else
  exec node  "$basedir/../delete-empty/bin/cli.js" "$@"
fi
