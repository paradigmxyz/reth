#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/@ethereumjs+rlp@4.0.1/node_modules/@ethereumjs/rlp/bin/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/@ethereumjs+rlp@4.0.1/node_modules/@ethereumjs/rlp/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/@ethereumjs+rlp@4.0.1/node_modules/@ethereumjs/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/@ethereumjs+rlp@4.0.1/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/@ethereumjs+rlp@4.0.1/node_modules/@ethereumjs/rlp/bin/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/@ethereumjs+rlp@4.0.1/node_modules/@ethereumjs/rlp/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/@ethereumjs+rlp@4.0.1/node_modules/@ethereumjs/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/@ethereumjs+rlp@4.0.1/node_modules:/Users/keszeyd/work/taiko-reth/packages/protocol/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/rlp" "$@"
else
  exec node  "$basedir/../../bin/rlp" "$@"
fi
